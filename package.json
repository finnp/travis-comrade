{
  "name": "travis-comrade",
  "version": "1.1.0",
  "description": "Status bar for travis",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-packager . 'TravisComrade' --platform=darwin --arch=x64 --version=0.36.7 --overwrite --icon img/travis.icns",
    "release": "electron-release --template notes.md --app TravisComrade-darwin-x64/TravisComrade.app --token $GH_TOKEN"
  },
  "keywords": [
    "travis",
    "helper",
    "menubar"
  ],
  "author": "Finn Pauls",
  "license": "ISC",
  "dependencies": {
    "delegate-dom": "0.0.1",
    "electron-default-menu": "^0.1.1",
    "fuzzy": "^0.1.1",
    "menubar": "^4.0.2",
    "request": "^2.69.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "electron-prebuilt": "^0.36.7",
    "electron-release": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finnp/travis-comrade.git"
  },
  "bugs": {
    "url": "https://github.com/finnp/travis-comrade/issues"
  },
  "homepage": "https://github.com/finnp/travis-comrade#readme"
}
